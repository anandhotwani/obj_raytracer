    "version": "2.0.0",
    "isShellCommand":true,
    "options": {
    "cwd":"${workspaceRoot}/build"
    },
    "tasks": [
    {
    "label": "cmake",
    "command": "cmake -G 'Unix Makefiles' -DCMAKE_BUILD_TYPE=Debug ..",
    "type": "shell",
    "presentation": {
    "echo": true,
    "reveal": "always",
    "panel": "shared"
    }
    },
    {
    "label": "make",
    "command":"make -j 8",
    "presentation": {
    "echo": true,
    "reveal": "always",
    "panel": "shared"
    },
    "isBuildCommand":true
    }
    ]
    }